{
  "hosting": {
    "site": "proclockout",
    "public": "build",
    "headers": 
      {
        "source": "/analytics",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "http://localhost:3000" 
          }
        
    ]
    ,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}}
